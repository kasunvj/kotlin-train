fun main() {    
   
    var mutableList = mutableListOf(
            arrayOf(1,2,3,4),
            arrayOf(5,6,7,8),
            arrayOf(9,10,11,12),
            arrayOf(13,14,15,16))
    
    for (i in 0..3){
        var mulist = mutableList[i].toMutableList() //array to list 
        mulist.shuffle()
        var shuffledArray = mulist.toTypedArray()
        println(shuffledArray.contentToString())
    }
}
